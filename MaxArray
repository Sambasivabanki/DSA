public class MaxArray {

    public static void main(String[] args) {
        int [] arr={10,34,465,7,45};
        System.out.println(maxval(arr));

    }
    static int maxval(int[] arr)
    {
        int max=arr[4];
        for(int i=0;i<arr.length;i++)
        {
            if(arr[i]>max){
                max=arr[i];
            }
        }
        return max;
    }
}
